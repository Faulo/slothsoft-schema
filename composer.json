{
    "name": "slothsoft/schema",
    "description": "A Farah module for processing XML Schema files.",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "slothsoft/amber": "dev-master",
        "slothsoft/core": "dev-master",
        "slothsoft/farah": "dev-master",
        "slothsoft/savegame": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Schema\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "farah-domain",
        "farah-module",
        "slothsoft"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
